module Graft
  class Callback
    type options = Hash[Symbol, untyped]

    @opts: options

    @block: untyped

    @enabled: bool

    attr_reader name: String

    def initialize: (?untyped? name, ?options opts) { () -> untyped } -> void

    def disable: () -> void

    def enable: () -> void

    def enabled?: () -> bool
  end
end
